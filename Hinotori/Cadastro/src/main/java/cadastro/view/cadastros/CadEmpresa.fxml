<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import com.jfoenix.controls.JFXTextField?>
<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<StackPane fx:id="spRoot" alignment="TOP_CENTER" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cadastro.controller.cadastros.CadEmpresaController">
   <children>
      <AnchorPane fx:id="apContainer" minHeight="0.0" minWidth="0.0">
         <children>
            <ScrollPane fx:id="spBackground" fitToHeight="true" fitToWidth="true" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <content>
                <AnchorPane fx:id="apContainerInterno" minHeight="520.0" minWidth="600.0" styleClass="background-cartao">
                     <children>
                        <HBox fx:id="hbTitulo" alignment="CENTER" prefHeight="200.0" styleClass="titBackound-empresa" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                           <children>
                              <Label alignment="CENTER" contentDisplay="CENTER" text="Cadastro de Empresa" textFill="WHITE" HBox.hgrow="ALWAYS">
                                 <font>
                                    <Font size="24.0" />
                                 </font>
                                 <effect>
                                    <DropShadow spread="0.5" />
                                 </effect>
                              </Label>
                           </children>
                        </HBox>
                        <VBox alignment="TOP_CENTER" maxHeight="1000.0" maxWidth="1200.0" minHeight="370.0" minWidth="650.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="150.0">
                           <children>
                              <VBox alignment="TOP_CENTER" maxHeight="800.0" maxWidth="1000.0" minHeight="350.0" minWidth="600.0" spacing="30.0" styleClass="background-dados">
                                 <padding>
                                    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                                 </padding>
                                 <effect>
                                    <DropShadow height="15.0" offsetX="1.0" offsetY="1.0" radius="7.0" width="15.0" />
                                 </effect>
                                 <children>
                                    <HBox alignment="CENTER" minHeight="30.0" prefHeight="40.0" prefWidth="388.0" spacing="5.0">
                                       <children>
                                          <JFXTextField fx:id="txtId" labelFloat="true" minHeight="30.0" onKeyPressed="#onTxtIdEnter" onMouseClicked="#onTxtIdClick" prefWidth="100.0" promptText="Id">
                                             <font>
                                                <Font name="Times New Roman" size="12.0" />
                                             </font>
                                             <tooltip>
                                                <Tooltip text="Valor 0 para um registro novo ou digite um código de cliente para pesquisar." />
                                             </tooltip>
                                          </JFXTextField>
                                          <HBox alignment="CENTER_RIGHT" spacing="5.0" HBox.hgrow="ALWAYS">
                                             <children>
                                                <JFXComboBox fx:id="cbSituacao" minHeight="30.0" prefWidth="120.0" promptText="Situação" />
                                             </children>
                                          </HBox>
                                       </children>
                                    </HBox>
                                    <HBox alignment="CENTER" spacing="10.0" VBox.vgrow="ALWAYS">
                                       <children>
                                          <VBox alignment="CENTER" spacing="10.0" HBox.hgrow="ALWAYS">
                                             <children>
                                                <HBox VBox.vgrow="ALWAYS">
                                                   <children>
                                                      <JFXTextField fx:id="txtRazaoSocial" labelFloat="true" promptText="Razão Social" HBox.hgrow="ALWAYS">
                                                         <font>
                                                            <Font name="Times New Roman" size="12.0" />
                                                         </font>
                                                      </JFXTextField>
                                                   </children>
                                                </HBox>
                                                <HBox VBox.vgrow="ALWAYS">
                                                   <children>
                                                      <JFXTextField fx:id="txtNomeFantasia" labelFloat="true" promptText="Nome Fantasia" HBox.hgrow="ALWAYS">
                                                         <font>
                                                            <Font name="Times New Roman" size="12.0" />
                                                         </font>
                                                      </JFXTextField>
                                                   </children>
                                                </HBox>
                                                <HBox VBox.vgrow="ALWAYS">
                                                   <children>
                                                      <JFXTextField fx:id="txtCnpj" labelFloat="true" promptText="CNPJ" HBox.hgrow="ALWAYS">
                                                         <font>
                                                            <Font name="Times New Roman" size="12.0" />
                                                         </font>
                                                      </JFXTextField>
                                                   </children>
                                                </HBox>
                                                <HBox alignment="CENTER" spacing="5.0" VBox.vgrow="ALWAYS">
                                                   <children>
                                                      <JFXButton fx:id="btnContato" buttonType="RAISED" onAction="#onBtnContatoClick" onKeyPressed="#onBtnContatoEnter" styleClass="btnContatos-empresa" text="Cad. Contato">
                                                         <graphic>
                                                            <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                                                               <image>
                                                                  <Image url="@../../resources/imagens/white/cadastros/btnContato_100.png" />
                                                               </image>
                                                            </ImageView>
                                                         </graphic>
                                                      </JFXButton>
                                                      <JFXButton fx:id="btnEndereco" buttonType="RAISED" onAction="#onBtnEnderecoClick" onKeyPressed="#onBtnEnderecoEnter" styleClass="btnEndereco-empresa" text="Cad. Endereço">
                                                         <graphic>
                                                            <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                                                               <image>
                                                                  <Image url="@../../resources/imagens/white/cadastros/btnEndereco_100.png" />
                                                               </image>
                                                            </ImageView>
                                                         </graphic>
                                                      </JFXButton>
                                                   </children>
                                                </HBox>
                                             </children>
                                          </VBox>
                                          <VBox alignment="CENTER" spacing="10.0">
                                             <children>
                                                <VBox alignment="CENTER" styleClass="background-imagens">
                                                   <children>
                                                      <ImageView fx:id="imgLogo" fitHeight="200.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                                                         <image>
                                                            <Image url="@../../resources/imagens/icon/icoPrincipal_100.png" />
                                                         </image>
                                                      </ImageView>
                                                   </children>
                                                   <padding>
                                                      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                                   </padding>
                                                   <VBox.margin>
                                                      <Insets />
                                                   </VBox.margin>
                                                </VBox>
                                                <HBox alignment="CENTER" spacing="10.0">
                                                   <children>
                                                      <JFXButton fx:id="btnExcluirImagem" buttonType="RAISED" onAction="#onBtnExcluirImagemClick" onKeyPressed="#onBtnExcluirImagemEnter" styleClass="btnExcluir-usuario" text="Excluir" textFill="WHITE" />
                                                      <JFXButton fx:id="btnProcurarImagem" buttonType="RAISED" onAction="#onBtnProcurarImagemClick" onKeyPressed="#onBtnProcurarImagemEnter" styleClass="btnProcurar-usuario" text="Procurar" textFill="WHITE" />
                                                   </children>
                                                </HBox>
                                             </children>
                                          </VBox>
                                       </children>
                                    </HBox>
                                 </children>
                                 <VBox.margin>
                                    <Insets bottom="20.0" left="20.0" right="20.0" />
                                 </VBox.margin>
                              </VBox>
                           </children>
                        </VBox>
                     </children>
                  </AnchorPane>
               </content>
            </ScrollPane>
            <HBox fx:id="hbTituloBotoes" alignment="CENTER_LEFT" layoutX="1.0" layoutY="1.0" maxHeight="60.0" minHeight="60.0" opacity="0.0" spacing="10.0" styleClass="titBackound-empresa" AnchorPane.leftAnchor="1.0" AnchorPane.rightAnchor="1.0" AnchorPane.topAnchor="1.0">
               <padding>
                  <Insets left="10.0" right="10.0" />
               </padding>
               <children>
                  <Label alignment="CENTER" contentDisplay="CENTER" text="Empresas" textFill="WHITE">
                     <font>
                        <Font size="24.0" />
                     </font>
                     <effect>
                        <DropShadow spread="0.5" />
                     </effect>
                  </Label>
               </children>
            </HBox>
            <HBox alignment="CENTER_RIGHT" maxHeight="60.0" minHeight="60.0" spacing="10.0" styleClass="paneBotoes-empresa" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <children>
                  <JFXButton fx:id="btnConfirmar" alignment="CENTER" maxHeight="35.0" maxWidth="120.0" minHeight="35.0" minWidth="120.0" onAction="#onBtnConfirmarClick" onKeyPressed="#onConfirmarKeyPress" prefHeight="35.0" prefWidth="120.0" ripplerFill="BLACK" text="Confirmar" textFill="WHITE">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </JFXButton>
                  <JFXButton fx:id="btnCancelar" alignment="CENTER" maxHeight="35.0" maxWidth="120.0" minHeight="35.0" minWidth="120.0" onAction="#onBtnCancelarClick" onKeyPressed="#onCancelarKeyPress" prefHeight="35.0" prefWidth="120.0" ripplerFill="BLACK" text="Cancelar" textFill="WHITE">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </JFXButton>
                  <JFXButton fx:id="btnExcluir" alignment="CENTER" maxHeight="35.0" maxWidth="120.0" minHeight="35.0" minWidth="120.0" onAction="#onBtnExcluirClick" onKeyPressed="#onExcluirKeyPress" prefHeight="35.0" prefWidth="120.0" ripplerFill="BLACK" text="Excluir" textFill="WHITE">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </JFXButton>
                  <JFXButton fx:id="btnVoltar" alignment="CENTER" contentDisplay="CENTER" graphicTextGap="10.0" maxHeight="35.0" maxWidth="120.0" minHeight="35.0" minWidth="120.0" onAction="#onBtnVoltarClick" onKeyPressed="#onVoltarKeyPress" prefHeight="35.0" prefWidth="120.0" ripplerFill="BLACK" text="Voltar" textFill="WHITE">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </JFXButton>
               </children>
               <padding>
                  <Insets left="150.0" right="20.0" />
               </padding>
            </HBox>
         </children>
      </AnchorPane>
   </children>
   <stylesheets>
      <URL value="@../../resources/css/Paleta_Cores.css" />
      <URL value="@../../resources/css/White_Geral.css" />
      <URL value="@../../resources/css/White_Cadastros.css" />
   </stylesheets>
</StackPane>
